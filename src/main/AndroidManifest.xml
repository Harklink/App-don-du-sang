<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfirstapp_2">

    <uses-permission android:name="android.permission.READ_CALENDAR"/>
    <uses-permission android:name="android.permission.WRITE_CALENDAR"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_accueil_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_accueil_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyFirstApp_2">
        <activity
            android:name=".Profil"
            android:label="@string/title_activity_profil"
            android:theme="@style/Theme.MyFirstApp_2.NoActionBar" />
        <activity
            android:name=".Login"
            android:label="Login"
            android:theme="@style/Theme.MyFirstApp_2.NoActionBar" />
        <activity
            android:name=".Register"
            android:label="Register"
            android:theme="@style/Theme.MyFirstApp_2.NoActionBar" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyFirstApp_2.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>